{
  "compilerOptions": {
    "types": ["@clerk/astro", "astro/client", "node"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "module": "es2020",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"]
    }
  },
  "include": ["src", ".astro/types.d.ts"],
  "exclude": ["dist", "node_modules"]
}
