        grid-template-columns: 1fr 1fr;
        gap: 12px;
      }
      @media (max-width: 720px) {
        form .row {
          grid-template-columns: 1fr;
        }
      }
      label {
        font-weight: 600;
        font-size: 14px;
        display: block;
        margin-bottom: 6px;
      }
      input,
      select,
      textarea {
        width: 100%;
        padding: 12px;
        border: 1px solid #d7dfec;
        border-radius: 10px;
        font-size: 15px;
        outline: none;
        background: #fff;
      }
      input:focus,
      select:focus,
      textarea:focus {
        border-color: var(--primary);
        box-shadow: 0 0 0 3px #1f3a8a22;
      }
      .help {
        font-size: 12px;
        color: var(--muted);
      }
      .error {
        color: var(--danger);
        font-size: 12px;
        margin-top: 6px;
      }
      .footer {
        padding: 28px 0;
        border-top: 1px solid #eef1f6;
