  <!-- Basic JS validation (replace TODO with your form endpoint if desired) -->
  <script>
    function validate(ids) {
      let ok = true;
      const name = document.getElementById(ids.name);
      const phone = document.getElementById(ids.phone);
      const email = document.getElementById(ids.email);
      const errName = document.getElementById(ids.errName);
      const errPhone = document.getElementById(ids.errPhone);
      const errEmail = document.getElementById(ids.errEmail);

      errName.textContent = '';
      errPhone.textContent = '';
      errEmail.textContent = '';

      if (!name.value.trim()) {
        errName.textContent = 'Please enter your name.';
        ok = false;
      }
      const phoneDigits = phone.value.replace(/\D/g, '');
      if (phoneDigits.length < 10) {
        errPhone.textContent = 'Please enter a valid phone number.';
        ok = false;
      }
      if (email.value && !/^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(email.value)) {
        errEmail.textContent = 'Please enter a valid email.';
        ok = false;
      }
      return ok;
    }

    function wireForm(formId, ids, alertId, successId) {
      const form = document.getElementById(formId);
      const alertBox = document.getElementById(alertId);
      const successBox = document.getElementById(successId);
      if (!form) return;

      form.addEventListener('submit', function (e) {
        e.preventDefault();
        alertBox.style.display = 'none';
        successBox.style.display = 'none';

        const ok = validate(ids);
        if (!ok) return;

        setTimeout(() => {
          successBox.style.display = 'block';
          form.reset();
        }, 400);
      });
    }

    wireForm(
      'contactForm',
      {
        name: 'name',
        phone: 'phone',
        email: 'email',
        errName: 'err-name',
        errPhone: 'err-phone',
        errEmail: 'err-email'
      },
      'formError',
      'formSuccess'
    );

    wireForm(
      'contactForm2',
      {
        name: 'name2',
        phone: 'phone2',
        email: 'email2',
        errName: 'err2-name',
        errPhone: 'err2-phone',
        errEmail: 'err2-email'
      },
      'form2Error',
      'form2Success'
    );
  </script>
  <script>
    (function () {
      const mobileHeader = document.getElementById('header-mobile');
      if (!mobileHeader) return;

      const btnMenu = mobileHeader.querySelector('.btn-menu');
      const canvasWrap = mobileHeader.querySelector('.canvas-nav-wrap');
      const overlay = mobileHeader.querySelector('.overlay-canvas-nav');
      const closeBtn = mobileHeader.querySelector('.canvas-menu-close');

      const toggleNav = (open) => {
        if (!canvasWrap) return;
        canvasWrap.classList.toggle('is-open', open);
        document.body.classList.toggle('nav-open', open);
        if (btnMenu) {
          btnMenu.setAttribute('aria-expanded', open ? 'true' : 'false');
        }
      };

      const handleToggle = () => {
        const isOpen = canvasWrap ? canvasWrap.classList.contains('is-open') : false;
        toggleNav(!isOpen);
      };

      if (btnMenu) {
        btnMenu.setAttribute('aria-expanded', 'false');
        btnMenu.addEventListener('click', handleToggle);
        btnMenu.addEventListener('keypress', (event) => {
          if (event.key === 'Enter' || event.key === ' ') {
            event.preventDefault();
            handleToggle();
          }
        });
      }

      if (overlay) {
        overlay.addEventListener('click', () => toggleNav(false));
      }
      if (closeBtn) {
        closeBtn.addEventListener('click', () => toggleNav(false));
        closeBtn.addEventListener('keypress', (event) => {
          if (event.key === 'Enter' || event.key === ' ') {
            event.preventDefault();
            toggleNav(false);
          }
        });
      }

      mobileHeader.querySelectorAll('.btn-submenu').forEach((btn) => {
        btn.setAttribute('aria-expanded', 'false');
        btn.addEventListener('click', (event) => {
          event.preventDefault();
          const parent = btn.closest('li');
          let isOpen = false;
          if (parent) {
            isOpen = parent.classList.toggle('menu-item-open');
          }
          btn.setAttribute('aria-expanded', isOpen ? 'true' : 'false');
        });
      });

      mobileHeader.querySelectorAll('.mainnav_canvas a').forEach((link) => {
        link.addEventListener('click', () => {
          toggleNav(false);
        });
      });
    })();
  </script>

