        display: block;
      }
      .container {
        width: min(1200px, 92%);
        margin: 0 auto;
      }
      .btn {
        display: inline-block;
        padding: 12px 18px;
        border-radius: 10px;
        font-weight: 600;
        transition: all 0.2s;
        border: 2px solid transparent;
        text-align: center;
        cursor: pointer;
      }
      .btn-primary {
        background: var(--primary);
        color: #fff;
      }
      .btn-primary:hover {
        background: var(--primary-dark);
        transform: translateY(-1px);
      }
      .btn-outline {
        border-color: var(--primary);
        color: var(--primary);
        background: transparent;
      }
      .btn-outline:hover {
        background: #eef3ff;
      }
      .badge {
        display: inline-flex;
        align-items: center;
        gap: 8px;
        padding: 6px 10px;
        border-radius: 999px;
        font-size: 12px;
        background: #eef3ff;
