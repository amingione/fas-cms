      .card {
        border: 1px solid #e9eef7;
        border-radius: 14px;
        padding: 18px;
        background: #fff;
      }
      .muted {
        color: var(--muted);
      }
      .kpi {
        display: flex;
        gap: 12px;
        align-items: center;
      }
